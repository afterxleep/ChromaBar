{
  "name": "chromabar",
  "displayName": "ChromaBar",
  "description": "Change title bar colors to distinguish VS Code windows per workspace",
  "version": "1.0.0",
  "publisher": "afterxleep",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/afterxleep/chromabar"
  },
  "bugs": {
    "url": "https://github.com/afterxleep/chromabar/issues"
  },
  "homepage": "https://github.com/afterxleep/chromabar#readme",
  "license": "MIT",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": ["Other", "Themes"],
  "keywords": ["workspace", "color", "titlebar", "theme", "window", "chromabar", "productivity", "visual"],
  "galleryBanner": {
    "color": "#4F7ABB",
    "theme": "dark"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "chromabar.selectColor",
        "title": "Select Workspace Color",
        "category": "ChromaBar"
      },
      {
        "command": "chromabar.resetColor",
        "title": "Reset Workspace Color",
        "category": "ChromaBar"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.0"
  }
}